#include"bits/stdc++.h"

using namespace std;

int main()
{
    int t, maximum;
    cin >> t;
    while(t--)
    {
        int n, a;
        cin >> n;
        vector<int>v;
        for(int i=0; i<n; i++)
        {
            cin >> a;
            v.push_back(a);
        }
        sort(v.begin(), v.end(), greater<int>());
        cout << v[0] << endl;
    }
    return 0;
}
