#include<iostream>

using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int x, y, z, a, b, c;
        cin >> x >> y >> z;
        a=min(x, y);
        b=min(x, z);
        c=min(y, z);
        
        int flag=0;
        if(x==max(a, b) && y==max(a, c) && z==max(b, c)) flag=1;

        if(flag==1)
        {
            cout << "YES" << endl;
            cout << a << " " << b << " " << c << endl;
        }
        else cout << "NO" << endl;
    }
    return 0;
}
