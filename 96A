#include<iostream>
#include<string>
#include<cstring>
using namespace std;

int main()
{
    string str;
    cin >> str;
    int cnt=1;
    for(int i=1; i<str.size(); i++)
    {
        if(str[i]==str[i-1]) cnt++;
        else cnt=1;

        if(cnt>=7) break;
    }
    if(cnt>=7) cout << "YES" << endl;
    else cout << "NO" << endl;
    return 0;
}
